<UserControl
    x:Class="ModuleB.Navigation.ViewBButton"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:inf="clr-namespace:Demo.Infrastructure;assembly=Demo.Infrastructure"
    xmlns:views="clr-namespace:ModuleB.Views">

    <Grid Margin="5">
        <Button
            Content="Navigate to View B"
            Command="{x:Static Member=inf:ApplicationCommands.NavigateCommand}"
            CommandParameter="{x:Type views:ViewB}"/>
    </Grid>

</UserControl>
